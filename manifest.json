{
  "manifest_version": 3,
  "name": "Custom Google Results Per Page",
  "version": "1.0",
  "description": "Customize how many results per page are shown on Google Search.",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "https://www.google.com/"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "options_page": "options.html",
  "author": "Kenan İLGÜN"
}
